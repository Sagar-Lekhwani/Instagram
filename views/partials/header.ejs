<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/remixicon@3.5.0/fonts/remixicon.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <link rel="stylesheet" href="../stylesheets/home.css">
  </head>
  <body>
    <div class="leftMenu">
      <div class="logo">
          <a href="#">
              <img class="instagramTextLogo" src="https://upload.wikimedia.org/wikipedia/commons/thumb/2/2a/Instagram_logo.svg/840px-Instagram_logo.svg.png" alt="">
          </a>
      </div>

      <!--LEFT MENU CODES HERE-->

      <ul id="leftMiddle">
          <li id="home" class="middleMenu">
              <i class="fa-solid fa-house"></i>
              <b>Home</b></li>
          <li id="search" class="middleMenu">
              <i class="fa-solid fa-magnifying-glass"></i>
              Search</li>
          <li id="discover" class="middleMenu">
              <i class="fa-regular fa-compass"></i>
              Explore</li>
          <li id="reels" class="middleMenu">
              <i class="fa-solid fa-clapperboard"></i>
              Reels</li>
          <li id="messages" class="middleMenu">
              <i class="fa-brands fa-facebook-messenger"></i>
              Messages</li>
          <li id="notifications" class="middleMenu">
              <i class="fa-regular fa-heart"></i>
              Notifications</li>
          <li id="create" class="middleMenu">
              <i class="fa-regular fa-square-plus"></i>
              Create</li>
          <li id="profile" class="middleMenu">
              <i class="fa-regular fa-user"></i>
              Profile</li>
          <li id="threads" class="bottomMenu">
                  <i class="fa-brands fa-threads"></i>
                  Threads</li>
          <li id="more" class="bottomMenu">
                  <i class="fa-solid fa-bars"></i>
                  More</li>
      </ul>
  </div>
    